LOGIC 1
   vector<int> soluiton;
   Node* temporary=head;
   int i=0;
   while(temporary!=NULL){
       soluiton.push_back(temporary->data);
       temporary=temporary->next;
   }
   int j=0;
   int n=soluiton.size();
   int k=n-1;
   while(j<k){
       if(soluiton[j]!=soluiton[k]){
           return false;
       }
       j++;
       k--;
   }
   return true;

